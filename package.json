{
  "name": "template",
  "version": "0.0.0",
  "description": "",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/main.js",
  "build": {
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "node_modules/",
      "package.json"
    ]
  },
  "scripts": {
    "start": "concurrently \"npm run start:main\" \"npm run start:web\"",
    "start:web": "webpack serve --config webpack.dev.js",
    "start:main": "wait-on http://localhost:3000 && npm-run-all --parallel hotbuild:main serve:main",
    "hotbuild:main": "webpack --watch --config webpack.main.js",
    "serve:main": "nodemon --exec \"electron\" ./dist/main.js",
    "test": "jest --watch",
    "test:ci": "jest",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "lint:ci": "eslint '*/**/*.{js,ts,tsx}'",
    "build:web": "webpack --config webpack.prod.js",
    "build:main": "tsc ./main/main.ts --outDir dist",
    "build": "rm -rf dist && npm run build:web && npm run build:main",
    "release": "electron-builder -mw",
    "release:win": "electron-builder -w",
    "release:mac": "electron-builder -m",
    "build:release": "npm run build && npm run release"
  },
  "author": "WWT - GIC",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "<rootDir>/test/setupTests.ts"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.6",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-loader": "^8.2.2",
    "concurrently": "^6.2.0",
    "css-loader": "^6.2.0",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "^2.3.2",
    "react-redux": "^7.2.4",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "source-map": "^0.7.3",
    "style-loader": "^3.2.1",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "typescript": "^4.3.5",
    "wait-on": "^6.0.0",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "electron-is-dev": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
